<%- include('header') %>
    
        <br/>
        <p class="h2"><%=title%></p>
        <table class="table">
            <thead>
                <tr>
                    <th scope="col">#</th>
                    <th scope="col">First-Name</th>
                    <th scope="col">Last-Name</th>
                    <th scope="col">Email</th>
                    <th scope="col">Operation</th>
                </tr>
            </thead>
            <tbody>
            <% for(let i = 0;i<user.length;i++){%>
                <tr>
                    <th scope="row"><%=i+1%></th>
                    <td><%=user[i].first_name %></td>
                    <td><%=user[i].last_name %></td>
                    <td><%=user[i].email %></td>
                    <% if(userData){%>
                    <% if(userData._id == user[i]._id){%>
                    <td><a href="/user/update/<%= user[i]._id %>" class="btn btn-info">Update/Delete</a></td>
                    <%}%>
                </tr>
                <% } %>
                <%}%>
            </tbody>
            </table>
            <% if(!userData){%>
            <a href="/user/register" class="btn btn-primary">Register Yourself</a>
            <%}%>
<%- include('footer')%>
